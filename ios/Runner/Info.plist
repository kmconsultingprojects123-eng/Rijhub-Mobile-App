<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>RijHub</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>Rijhub</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>

	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<!-- Existing app URL schemes (keep these) and add Google reversed client id entry below -->
	<key>CFBundleURLTypes</key>
	<array>
		<!-- Existing app URL scheme -->
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLName</key>
			<string>rijhub.com</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>rijhub</string>
			</array>
		</dict>

		<!-- Google Sign-In URL scheme: REPLACE the string below with your reversed iOS client id
		     (format: com.googleusercontent.apps.YOUR_CLIENT_ID). If you have a GoogleService-Info.plist
		     file in your Xcode project, this entry should match the REVERSED_CLIENT_ID value from it. -->
		<dict>
			<key>CFBundleTypeRole</key>
			<string>Editor</string>
			<key>CFBundleURLName</key>
			<string>com.googleusercontent.apps</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>com.googleusercontent.apps.YOUR_REVERSED_CLIENT_ID</string>
			</array>
		</dict>
	</array>

	<!-- Google Sign-In client id (web or iOS client id). Replace this with your actual client id. -->
	<key>GIDClientID</key>
	<string>YOUR_IOS_CLIENT_ID.apps.googleusercontent.com</string>

	<!-- Flutter deep linking flag -->
	<key>FlutterDeepLinkingEnabled</key>
	<true/>

	<!-- Allow the app to query other browser apps (required by AppAuth / external user agents) -->
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>googlechromes</string>
		<string>googlechrome</string>
		<string>firefox</string>
		<string>opera-https</string>
	</array>


	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<!-- Development-only: allow HTTP connections to local backend during development. Remove before production. -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
	</dict>


	<!-- Mapbox removed: MGLMapboxAccessToken entry deleted. The app now uses Google Maps; set the
	     Google API key via `GoogleService-Info.plist` (iOS) or runtime initialization. -->

	<!-- Google Maps API key for iOS. This is required by the native Google Maps SDK. -->
	<key>GMSApiKey</key>
	<string>AIzaSyAjdDayJe-TVV8Cz6a-kHFCoJm6oNgeix8</string>

	<!-- Location usage description (if you plan to enable user location) -->
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>App needs location access to show nearby artisans.</string>

	<key>LSRequiresIPhoneOS</key>
	<true/>

	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<false/>

	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
</dict>
</plist>
